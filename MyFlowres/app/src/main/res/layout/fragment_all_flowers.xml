        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/AllFlowers_Rec"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:forceDarkAllowed=""/>
    </LinearLayout>


</layout>